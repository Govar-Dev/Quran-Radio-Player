        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "vue": "^2.6.11",
        "vue-howler": "^0.7.0",
        "vue-lodash": "^2.1.2",
        "vue-scrollto": "^2.20.0",
        "vue-switches": "^2.0.1",
        "w-audioplayer-vue": "^2.0.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
